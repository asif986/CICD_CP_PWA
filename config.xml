<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vjd.cpNew" version="1.13" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>VJ Channel Partner</name>
    <description>An awesome Ionic/Cordova app.</description>
    <author email="hi@ionicframework.com" href="http://ionicframework.com/">Ionic Framework Team</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <feature name="CDVWKWebViewEngine">
        <param name="ios-package" value="CDVWKWebViewEngine" />
    </feature>
    <preference name="CordovaWebViewEngine" value="CDVWKWebViewEngine" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="FadeSplashScreenDuration" value="100" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreenDelay" value="100" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="ShowSplashScreen" value="true" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="orientation" value="portrait" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <platform name="android">
        <preference name="android-targetSdkVersion" value="29" />
        <resource-file src="google-services.json" target="app/google-services.json" />
        <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application" xmlns:android="http://schemas.android.com/apk/res/android">
            <application android:networkSecurityConfig="@xml/network_security_config" />
        </edit-config>
        <resource-file src="resources/android/xml/network_security_config.xml" target="app/src/main/res/xml/network_security_config.xml" />
        <allow-intent href="market:*" />
        <icon density="ldpi" src="resources/android/icon/icon-ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/icon-mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/icon-hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/icon-xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/icon-xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/icon-xxxhdpi.png" />
        <splash density="land-ldpi" src="resources/android/splash/cp-splash-ldpi.png" />
        <splash density="land-mdpi" src="resources/android/splash/cp-splash-mdpi.png" />
        <splash density="land-hdpi" src="resources/android/splash/cp-splash-hdpi.png" />
        <splash density="land-xhdpi" src="resources/android/splash/cp-splash-xhdpi.png" />
        <splash density="land-xxhdpi" src="resources/android/splash/cp-splash-xxhdpi.png" />
        <splash density="land-xxxhdpi" src="resources/android/splash/cp-splash-xxxhdpi.png" />
        <splash density="port-ldpi" src="resources/android/splash/cp-splash-ldpi.png" />
        <splash density="port-mdpi" src="resources/android/splash/cp-splash-mdpi.png" />
        <splash density="port-hdpi" src="resources/android/splash/cp-splash-hdpi.png" />
        <splash density="port-xhdpi" src="resources/android/splash/cp-splash-xhdpi.png" />
        <splash density="port-xxhdpi" src="resources/android/splash/cp-splash-xxhdpi.png" />
        <splash density="port-xxxhdpi" src="resources/android/splash/cp-splash-xxhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="resources/ios/icon/57.png" width="57" />
        <icon height="114" src="resources/ios/icon/114.png" width="114" />
